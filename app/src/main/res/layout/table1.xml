            android:layout_width="16dp"
            android:layout_height="16dp"/>

        <ImageView
            android:id="@+id/fi28"
            android:layout_marginTop="560dp"
            android:layout_marginEnd="25dp"
            android:layout_alignParentEnd="true"
            android:padding="0dp"
            app:srcCompat="@drawable/null17"
            android:layout_width="16dp"
            android:layout_height="16dp"/>

        <ImageView
            android:id="@+id/fi27"
